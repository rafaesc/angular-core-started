{
  "overrides": [
    {
      "files": "*/**/*.ts",
      "options": {
        "singleQuote": true,
        "printWidth": 100,
        "trailingComma": "none"
      }
    },
    {
      "files": "*/**/*.html",
      "options": {
        "proseWrap": "always",
        "printWidth": 50
      }
    }
  ]
}
